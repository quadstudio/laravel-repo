{
    "name": "quadstudio/laravel-repo",
    "description": "Repository for Laravel 5",
    "keywords": ["laravel", "repository"],
    "homepage": "https://github.com/quadstudio/laravel-repo",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Shuleyko",
            "email": "salex009@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "QuadStudio\\Repo\\": "src/Repo/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "5.6.*",
        "illuminate/console": "5.6.x",
        "illuminate/view": "5.6.*",
        "quadstudio/laravel-tools": "dev-master"
    },
    "extra": {
        "laravel": {
            "providers": [
                "QuadStudio\\Repo\\RepoServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true
    }
}
